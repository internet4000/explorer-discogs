<p><em>master</em></p>
<h1>{{model.title}}</h1>

<pre>year: {{model.year}}
numForSale: {{model.numForSale}}
lowestPrice: {{model.lowestPrice}}
</pre>

<h3>Genres</h3>
{{each-list list=model.genres}}

<h3>Styles</h3>
{{each-list list=model.styles}}

<h3>Tracklist</h3>
{{#each-list list=model.tracklist as |track|}}
  {{track.position}} -
  {{track.title}}
  {{track.duration}}
  {{track.type}}
  {{#each-list list=track.extraartists as |artist|}}
    {{link-to artist.name "artist" artist.id}} ({{artist.role}})
  {{/each-list}}
{{/each-list}}

<h3>Videos</h3>
{{#each-list list=model.videos as |video|}}
  <a href="{{video.uri}}">{{video.title}}</a>
{{/each-list}}

<h3>Artists</h3>
{{#each-list list=model.artists as |artist|}}
  {{link-to artist.name "artist" artist.id}}
{{/each-list}}

<h3>Main release</h3>
<ul>
  <li>{{link-to "View main release" "release" model.mainRelease.id}}</li>
  <li><a href="{{model.versionsUrl}}" rel="noopener" target="_blank">View more versions</a></li>
</ul>

{{outlet}}

<p>
  {{link-to-discogs url=model.discogsHumanUrl}}
</p>
