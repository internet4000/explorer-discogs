<h1>{{model.title}}</h1>

<pre>
  id: {{model.id}}
  country: {{model.country}}
  released: {{model.released}}
  uri: {{model.uri}}
  year: {{model.year}}
</pre>

<p>Genres:</p>
{{each-list list=model.genres}}

<p>Images:</p>
{{each-list list=model.images}}

<p>Styles:</p>
{{each-list list=model.styles}}

<p>Tracklist:</p>
{{#each-list list=model.tracklist as |track|}}
  {{track.position}} -
  {{track.title}}
  {{track.duration}}
  {{track.type}}
{{/each-list}}

<p>Videos</p>
{{each-list list=model.videos}}

{{outlet}}

