{{#each-list list=model.tracklist as |track|}}
  {{track.position}} -
  {{track.title}}
  {{track.duration}}
  {{track.type}}
  {{#each-list list=track.extraartists as |artist|}}
    {{link-to artist.name "artist" artist.id}} ({{artist.role}})
  {{/each-list}}
{{/each-list}}

<p>
  Videos
  <span>{{button-play-all playlist=model.videos}}</span>
</p>
{{#each-list list=model.videos as |video|}}
  {{track-video model=video}}
{{/each-list}}

{{outlet}}